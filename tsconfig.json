{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "noEmitOnError": true,
    "sourceMap": true,
    "importHelpers": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
